  </setupTask>
  <stream name="master"
      label="master">
    <setupTask
        xsi:type="setup:EclipseIniTask"
        option="-Doomph.redirection.terminologySetup"
        value="=https://raw.githubusercontent.com/nittka/terminology/master/terminologyDev.setup->${git.clone.terminology.location|uri}/terminologyDev.setup"
        vm="true"/>
    <description>master</description>
  </stream>
  <description>
